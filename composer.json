{
  "name": "laravel/laravel",
  "type": "project",
  "version": "0.9.0",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "barryvdh/laravel-dompdf": "^v0.9.0",
    "facade/ignition": "^2.9",
    "fideloper/proxy": "^4.4.1",
    "guzzlehttp/guzzle": "^7.3",
    "intervention/image": "^2.5.1",
    "jeremykenedy/laravel-roles": "^v6.0.0",
    "kwn/number-to-words": "^2.0.0",
    "laravel/framework": "^8.0",
    "laravel/tinker": "^v2.6.1",
    "spatie/laravel-image-optimizer": "^1.4"
  },
  "require-dev": {
    "symfony/css-selector": "^v5.3.4",
    "beyondcode/laravel-dump-server": "^1.7.0",
    "doctrine/dbal": "^3.1.0",
    "laravel/ui": "^3.0",
    "mockery/mockery": "^1.4.3",
    "nunomaduro/collision": "^v5.4.0",
    "nunomaduro/larastan": "^v0.7.5",
    "phpunit/phpunit": "^9.5.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ]
  }
}
