{
  "name": "laravel/laravel",
  "type": "project",
  "version": "0.9.0",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "barryvdh/laravel-dompdf": "^0.8.5",
    "fideloper/proxy": "^4.0",
    "intervention/image": "^2.5",
    "jeremykenedy/laravel-roles": "^2.3",
    "kwn/number-to-words": "^1.9",
    "laravel/framework": "7.0",
    "laravel/tinker": "^2.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1.39",
    "spatie/laravel-image-optimizer": "^1.4",
    "facade/ignition": "^2.0",
    "laravel/ui": "^2.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "doctrine/dbal": "^2.9",
    "fzaninotto/faker": "^1.4",
    "laravel/homestead": "^11.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.1",
    "nunomaduro/larastan": "^0.6.4",
    "phpunit/phpunit": "^9",
    "symfony/css-selector": "^5.1.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  }
}
